{
  "expo": {
    "name": "influmart",
    "slug": "influmart",
    "description": "One point solution for influencers-brands.",
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/influmart_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.influmart.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.influmart.app",
      "versionCode": 6,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "abaa333a-85b0-439d-879f-db89ac029eab"
      }
    },
    "owner": "influmart",
    "scheme": "influmart"
  }
}
